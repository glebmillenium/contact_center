<Window x:Class="contact_center_application.form.UploadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:contact_center_application.form"
        mc:Ignorable="d"
        Title="Отправление файла" Height="136.34" Width="316.337">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="0.1*"></RowDefinition>
			<RowDefinition Height="0.15*"></RowDefinition>
			<RowDefinition Height="0.2*"></RowDefinition>
		</Grid.RowDefinitions>

		<TextBlock x:Name="nameFile" Grid.Row="0" Margin="5" TextAlignment="Center"></TextBlock>
		<ProgressBar x:Name="progressBar" Grid.Row="1" Margin="5"></ProgressBar>
		<TextBlock x:Name="info" Grid.Row="2" Margin="5" TextAlignment="Center" TextWrapping="Wrap"></TextBlock>
	</Grid>
</Window>
