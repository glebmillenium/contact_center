<Window x:Class="Patcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Patcher"
        mc:Ignorable="d"
        Title="Обновление" Height="200" Width="350"
			MinWidth="350"
			MaxWidth="700"
			MinHeight="200"
			MaxHeight="400">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="1.2*"></RowDefinition>
			<RowDefinition Height="1.3*"></RowDefinition>
			<RowDefinition Height="2*"></RowDefinition>
		</Grid.RowDefinitions>
		
		<StackPanel Margin="7" Grid.Row="0">
			
		</StackPanel>
		
		<ProgressBar x:Name="bar" Margin="10" Grid.Row="1"></ProgressBar>
		<TextBlock HorizontalAlignment="Center" x:Name="info" Grid.Row="2"></TextBlock>

	</Grid>
</Window>
